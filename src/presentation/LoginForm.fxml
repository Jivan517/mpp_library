<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox id="vpane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.LoginForm">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="288.0" text="Welcome to Librarian System" />
      <HBox alignment="CENTER" depthTest="DISABLE" prefHeight="52.0" prefWidth="289.0">
         <children>
            <Label alignment="CENTER" text="Username:" />
            <TextField id="username" prefHeight="12.0" prefWidth="112.0" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
      <HBox alignment="CENTER" prefHeight="37.0" prefWidth="185.0">
         <children>
            <Label text=" Password:" />
            <TextField id="password" prefHeight="27.0" prefWidth="113.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="65.0" prefWidth="254.0">
         <children>
            <Button id="login" mnemonicParsing="false" onAction="#handleLoginButtonAction" text="Login">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin></Button>
            <Button id="reset" mnemonicParsing="false" onAction="#handleResetButtonAction" text="Reset" />
         </children>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </opaqueInsets>
</VBox>
