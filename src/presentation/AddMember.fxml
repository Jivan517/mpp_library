<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.AddMember">
   <children>
      <VBox prefHeight="40.0" prefWidth="414.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Add Library Member to System">
               <VBox.margin>
                  <Insets left="80.0" top="20.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="414.0">
         <children>
            <Label prefHeight="17.0" prefWidth="98.0" text="Member ID">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="txtmember_id">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="17.0" prefWidth="98.0" text="First Name" />
            <TextField fx:id="txtFirstName" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="17.0" prefWidth="98.0" text="Last Name" />
            <TextField fx:id="txtLastName" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="17.0" prefWidth="98.0" text="Street" />
            <TextField fx:id="txtStreet" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="17.0" prefWidth="98.0" text="City" />
            <TextField fx:id="txtCity" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="17.0" prefWidth="98.0" text="State" />
            <TextField fx:id="txtState" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="17.0" prefWidth="98.0" text="zip" />
            <TextField fx:id="txtZip" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="95.0" prefWidth="414.0">
         <children>
            <Label prefHeight="17.0" prefWidth="98.0" text="Telephone No." />
            <TextField fx:id="txtPhone" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleSubmitButtonAction" text="Submit">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleResetButtonAction" text="Reset" />
         </children>
      </HBox>
   </children>
</VBox>
