<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="360.0" prefWidth="546.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.AddCopyForm">
   <children>
      <HBox>
         <children>
            <Region nodeOrientation="LEFT_TO_RIGHT" HBox.hgrow="ALWAYS" />
            <Label prefHeight="25.0" prefWidth="74.0" text="Book ISBN: " />
            <TextField fx:id="isbnText" promptText="ISBN" />
            <Button mnemonicParsing="false" onAction="#handleSearchButtonAction" text="Search" />
            <Region HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="83.0" text="ISBN" textAlignment="CENTER" />
                  <Label fx:id="isbn" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="100.0" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="83.0" text="ID" textAlignment="CENTER" />
                  <Label fx:id="bid" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="100.0" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="83.0" text="Title" textAlignment="CENTER" />
                  <Label fx:id="title" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="100.0" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="83.0" text="Popular" textAlignment="CENTER" />
                  <Label fx:id="popular" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="100.0" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="83.0" text="Author" textAlignment="CENTER" />
                  <Label fx:id="author" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="100.0" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="83.0" text="CopyNum" textAlignment="CENTER" />
                  <Label fx:id="copynum" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="100.0" />
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#handleAddButtonAction" text="Add copy" />
         </children>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
