<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="360.0" prefWidth="546.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <children>
      <HBox>
         <children>
            <Region nodeOrientation="LEFT_TO_RIGHT" HBox.hgrow="ALWAYS" />
            <Label prefHeight="25.0" prefWidth="74.0" text="Book ISBN: " />
            <TextField promptText="ISBN" />
            <Button mnemonicParsing="false" text="Search" />
            <Region HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TableView prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
              <columns>
                  <TableColumn prefWidth="75.0" text="Title" />
                <TableColumn prefWidth="75.0" text="ISBN" />
                <TableColumn prefWidth="75.0" text="ID" />
                  <TableColumn prefWidth="75.0" text="Available" />
                  <TableColumn prefWidth="75.0" text="Authors" />
                  <TableColumn prefWidth="75.0" text="Popularity" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
      <HBox alignment="CENTER" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS" />
            <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox alignment="CENTER">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" text="Add copy" />
         </children>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
